{
    "version": 2,
    "name": "finance-manager",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/finance-manager/browser"
            }
        },
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/auth/(.*)",
            "dest": "server/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "server/index.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}